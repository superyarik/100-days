{
  "expo": {
    "name": "Hundred Habits",
    "slug": "hundred-habits",
    "version": "1.7.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.westmorelandcreative.hundredhabits",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFD700"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.westmorelandcreative.hundredhabits",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Разрешите этому приложению собирать данные, связанные с приложением, которые могут использоваться для отслеживания вас или вашего устройства и показа персонализированной рекламы."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFD700"
      },
      "package": "ru.superyarik.hundredhabits"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      ["expo-localization"],
      ["@morrowdigital/watermelondb-expo-plugin"],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "kotlinVersion": "1.6.10",
            "packagingOptions": {
              "pickFirst": ["**/libc++_shared.so"]
            },
            "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
          }
        }
      ],
      "expo-localization",
      "expo-font",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "Разрешите этому приложению собирать данные, связанные с приложением, которые могут использоваться для отслеживания вас или вашего устройства и показа персонализированной рекламы."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "22a649d2-a170-43d9-9696-920b1d363098"
      }
    },
    "owner": "superyarik",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/22a649d2-a170-43d9-9696-920b1d363098"
    }
  }
}
